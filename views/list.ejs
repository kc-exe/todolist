<%- include("header") -%>
<div class="box" id="heading">
  <h1> <%= kindOfDay %> </h1>
</div>
<div class="box">
  <% for(var i=0;i<itemNames.length;i++) { %>
  <div class="item">
    <input type="checkbox">
    <p><%= itemNames[i] %> </p>

  </div>
  <%  }  %>
  <form class="item" action="/" method="post">

    <input type="text" name="newInput" placeholder="New Item" autocomplete="off">

    <button type="submit" name="submit">+</button>

  </form>
</div>

<%- include("footer") -%>
